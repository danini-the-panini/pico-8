pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
level=1
complete=false
dead=true
timeup=false
starttime=0
maxtime=100
timedone=0
totalscore=0
px=8
py=7
chips=11
inv={}
hint={
"collect chips to get past the",
"chip socket. use keys to open",
"doors"
}

function _init()
  starttime=time()
end

function cleartile(x,y)
  mset(x*2,y*2,1)
  mset(x*2+1,y*2,2)
  mset(x*2,y*2+1,17)
  mset(x*2+1,y*2+1,18)
end

function find(t,v)
  for x in all(t) do
    if x==v then
      return x
    end
  end
  return nil
end

function restart()
		complete=false
		dead=false
		timeup=false
		starttime=time()
		px=8
		py=7
		chips=11
		inv={}
end

function _update()
  if timeleft()<=0 then
    timeup=true
  end
  
  if timeup or dead then
    if btnp(❎) or btnp(🅾️) then
      restart()
    end
    return
  end
  
  if complete then
    if btnp(❎) or btnp(🅾️) then
      restart()
    end
    return
  end

  local npx=px
  local npy=py
  if btnp(⬅️) then
    npx-=1
  elseif btnp(➡️) then
    npx+=1
  end
  if btnp(⬆️) then
    npy-=1
  elseif btnp(⬇️) then
    npy+=1
  end
  local m = mget(npx*2,npy*2)
  if m==1 or m==13 then
    px=npx
    py=npy
  elseif m==5 then
    chips-=1
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==32 then
    add(inv,"redkey")
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==36 then
    add(inv,"greenkey")
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==40 then
    add(inv,"bluekey")
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==44 then
    add(inv,"yellowkey")
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==34 and del(inv,"redkey") then
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==38 and find(inv,"greenkey") then
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==42 and del(inv,"bluekey") then
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==46 and del(inv,"yellowkey") then
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==7 and chips==0 then
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==9 then
    px=npx
    py=npy
    timedone=timeleft()
    totalscore+=timescore()+levelscore()
    complete=true
  end
end

function drawbox(x,y,w,h)
  line(x-1,y-1,x+w+1,y-1,7)
  line(x-1,y-1,x-1,y+h+1,7)
  line(x+w+1,y-1,x+w+1,y+h+1,5)
  line(x-1,y+h+1,x+w+1,y+h+1.5)
  rectfill(x,y,x+w,y+h,6)
end

function drawinbox(x,y,w,h)
  line(x-1,y-1,x+w+1,y-1,5)
  line(x-1,y-1,x-1,y+h+1,5)
  line(x+w+1,y-1,x+w+1,y+h+1,7)
  line(x-1,y+h+1,x+w+1,y+h+1.7)
  rectfill(x,y,x+w,y+h,0)
end

function printn(s,x,y)
  printnm(s,x,y,0)
end

function printnm(s,x,y,m)
  local c1=3
  local c2=11
  if s<=m then
    c1=4
    c2=10
  end
  if s < 10 then
    print("00",x,y,c1)
    print(s,x+8,y,c2)
  elseif s < 100 then
    print("0",x,y,c1)
    print(s,x+4,y,c2)
  else
    print(s,x,y,c2)
  end
end

function invspr(n)
  if n <= #inv then
    if inv[n] == "redkey" then
      return 65
    elseif inv[n] == "greenkey" then
      return 66
    elseif inv[n] == "bluekey" then
      return 67
    elseif inv[n] == "yellowkey" then
      return 68
    end
  else
    return 64
  end
end

function timeleft()
  if complete then
    return timedone
  end
  return max(maxtime-flr(time()-starttime), 0)
end

function timescore()
  return timedone*10
end

function levelscore()
  return level*500
end

function _draw()
  rectfill(0, 0, 128, 128, 0)
  map(px*2-7, py*2-7, 0, 0, 16, 16)
  spr(11, 56, 56, 2, 2)
  drawbox(0,112,128,16)
  print("level",3,119,0)
  print("level",2,118,8)
  drawinbox(25,116,14,8)
  printn(level,27,118)
  spr(70,42,116)
  drawinbox(52,116,14,8)
  printnm(timeleft(),54,118,10)
  spr(69,68,116)
  drawinbox(78,116,14,8)
  printn(chips,80,118)
  rectfill(96,112,128,128,0)
  spr(invspr(1),96,112)
  spr(invspr(2),104,112)
  spr(invspr(3),112,112)
  spr(invspr(4),120,112)
  spr(invspr(5),96,120)
  spr(invspr(6),104,120)
  spr(invspr(7),112,120)
  spr(invspr(8),120,120)
  
  if px==8 and py==6 then
    drawinbox(4,84,120,24)
    print(hint[1],6,86,12)
    print(hint[2],6,94,12)
    print(hint[3],6,102,12)
  end
  
  if complete then
    rectfill(14,14,112,80,1)
    print("level complete!", 36, 16, 7)
    drawbox(18,24,90,52)
    print("go bit buster!", 38, 26, 0)
    print("time bonus: "..timescore(), 38, 36, 0)
    print("level bonus: "..levelscore(), 34, 42, 0)
    print("level score: "..(timescore()+levelscore()), 34, 48, 0)
    print("total score: "..totalscore, 34, 54, 0)
    drawbox(44, 64, 38, 8)
    print("onward!❎", 46, 66, 0)
  end
  
  if timeup then
    rectfill(20,32,106,70,1)
    print("chip's challenge", 32, 34, 7)
    drawbox(24,42,78,24)
    print("ooops! out of time!", 26, 44,0)
    drawbox(51, 54, 24, 8)
    print("ok ❎", 54, 56, 0)
  end
  
  if dead then
    rectfill(20,32,106,70,1)
    print("chip's challenge", 32, 34, 7)
    drawbox(24,42,78,24)
    print("ooops! chips died!", 28, 44,0)
    drawbox(51, 54, 24, 8)
    print("ok ❎", 54, 56, 0)
  end
end
