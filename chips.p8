pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
spawns={
  {8,7},
  {31,6}
}
levelchips={11,4}
level=1
complete=false
dead=false
deathmsg={}
timeup=false
starttime=0
maxtime=100
timedone=0
totalscore=0
px=8
py=7
chips=11
inv={}
hints={
 [8]={[6]={
		 "collect chips to get past the",
		 "chip socket. use keys to open",
		 "doors"
	}},
	[32]={[6]={
	  "push blocks into water to",
	  "make dirt. watch out for",
	  "monsters"
	}}
}
mspawns={
  {},
  {
    {21,4,70,2},
    {21,6,70,2},
    {21,8,70,2},
  }
}
monsters={}
ticklen=0.25

function die(msg)
  timedone=timeleft()
  dead=true
  deathmsg=msg
end

function win()
  timedone=timeleft()
  complete=true
end

function timeout()
  timedone=timeleft()
  timeup=true
end

function gettile(x,y)
  return mget(x*2,y*2)
end

function dup(tbl)
  if type(tbl)!="table" then
    return tbl
  end
  local t={}
  for k,v in pairs(tbl) do
    t[k]=dup(v)
  end
  return t
end

function _init()
		restart()
end

function settile(x,y,n)
  mset(x*2,y*2,n)
  mset(x*2+1,y*2,n+1)
  mset(x*2,y*2+1,n+16)
  mset(x*2+1,y*2+1,n+17)
end

function cleartile(x,y)
  settile(x,y,1)
end

function find(t,v)
  for x in all(t) do
    if x==v then
      return x
    end
  end
  return nil
end

function restart()
		complete=false
		dead=false
		timeup=false
		starttime=time()
		lasttick=time()
		px=spawns[level][1]
		py=spawns[level][2]
		chips=levelchips[level]
		inv={}
		monsters=dup(mspawns[level])
		reload(0x1000, 0x1000, 0x2000)
end

function nextlevel()
		level+=1
		restart()
end

function update_spider(spider)
  mx=spider[1]
  my=spider[2]
  mr=spider[4]
  if mr==0 then
    if gettile(mx-1,my)==1 then
      spider[1]-=1
      spider[4]=3
    elseif gettile(mx,my-1)==1 then
      spider[2]-=1
    elseif gettile(mx+1,my)==1 then
      spider[1]+=1
      spider[4]=1
    end
  elseif mr==1 then
    if gettile(mx,my-1)==1 then
      spider[2]-=1
      spider[4]=0
    elseif gettile(mx+1,my)==1 then
      spider[1]+=1
    elseif gettile(mx,my+1)==1 then
      spider[2]+=1
      spider[4]=2
    end
  elseif mr==2 then
    if gettile(mx+1,my)==1 then
      spider[1]+=1
      spider[4]=1
    elseif gettile(mx,my+1)==1 then
      spider[2]+=1
    elseif gettile(mx-1,my)==1 then
      spider[1]-=1
      spider[4]=3
    end
  elseif mr==3 then
    if gettile(mx,my+1)==1 then
      spider[2]+=1
      spider[4]=2
    elseif gettile(mx-1,my)==1 then
      spider[1]-=1
    elseif gettile(mx,my-1)==1 then
      spider[2]-=1
      spider[4]=0
    end
  end
end

function _tick()
  for mon in all(monsters) do
    ms=mon[3]
    if ms==70 then
      update_spider(mon)
    end
    if mon[1]==px and mon[2]==py then
      die({
        "look out for",
        "creatures!"
      })
    end
  end
end

function _update()
  if timeleft()<=0 then
    timeout()
  end
  
  if timeup or dead then
    if btnp(‚ùé) or btnp(üÖæÔ∏è) then
      restart()
    end
    return
  end
  
  if complete then
    if btnp(‚ùé) or btnp(üÖæÔ∏è) then
      nextlevel()
    end
    return
  end
  
  if time()-lasttick>ticklen then
    lasttick=time()
    _tick()
  end

  local npx=px
  local npy=py
  local up=false
  local down=false
  local left=false
  local right=false
  if btnp(‚¨ÖÔ∏è) then
    npx-=1
    left=true
  elseif btnp(‚û°Ô∏è) then
    npx+=1
    right=true
  end
  if btnp(‚¨ÜÔ∏è) then
    npy-=1
    up=true
  elseif btnp(‚¨áÔ∏è) then
    npy+=1
    down=true
  end
  local m = gettile(npx,npy)
  if m==1 or m==13 then
    px=npx
    py=npy
  elseif m==5 then
    chips-=1
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==32 then
    add(inv,"redkey")
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==36 then
    add(inv,"greenkey")
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==40 then
    add(inv,"bluekey")
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==44 then
    add(inv,"yellowkey")
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==34 and del(inv,"redkey") then
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==38 and find(inv,"greenkey") then
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==42 and del(inv,"bluekey") then
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==46 and del(inv,"yellowkey") then
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==7 and chips==0 then
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==9 then
    px=npx
    py=npy
    totalscore+=timescore()+levelscore()
    win()
  elseif m==64 then
    local nbx=npx
    local nby=npy
    if up then
      nby-=1
    elseif down then
      nby+=1
    elseif left then
      nbx-=1
    elseif right then
      nbx+=1
    end
    mb=gettile(nbx,nby)
    if mb==1 or mb==13 then
      cleartile(npx,npy)
      settile(nbx,nby,64)
      px=npx
      py=npy
    elseif mb==66 then
      cleartile(npx,npy)
      settile(nbx,nby,68)
      px=npx
      py=npy
    end
  elseif m==68 then
    cleartile(npx,npy)
    px=npx
    py=npy
  elseif m==66 then
    px=npx
    py=npy
    die({
      "chips can't swim",
      "without flippers!"
    })
  end
end

function drawbox(x,y,w,h)
  line(x-1,y-1,x+w+1,y-1,7)
  line(x-1,y-1,x-1,y+h+1,7)
  line(x+w+1,y-1,x+w+1,y+h+1,5)
  line(x-1,y+h+1,x+w+1,y+h+1.5)
  rectfill(x,y,x+w,y+h,6)
end

function drawinbox(x,y,w,h)
  line(x-1,y-1,x+w+1,y-1,5)
  line(x-1,y-1,x-1,y+h+1,5)
  line(x+w+1,y-1,x+w+1,y+h+1,7)
  line(x-1,y+h+1,x+w+1,y+h+1.7)
  rectfill(x,y,x+w,y+h,0)
end

function printn(s,x,y)
  printnm(s,x,y,0)
end

function printnm(s,x,y,m)
  local c1=3
  local c2=11
  if s<=m then
    c1=4
    c2=10
  end
  if s < 10 then
    print("00",x,y,c1)
    print(s,x+8,y,c2)
  elseif s < 100 then
    print("0",x,y,c1)
    print(s,x+4,y,c2)
  else
    print(s,x,y,c2)
  end
end

function invspr(n)
  if n <= #inv then
    if inv[n] == "redkey" then
      return 250
    elseif inv[n] == "greenkey" then
      return 251
    elseif inv[n] == "bluekey" then
      return 252
    elseif inv[n] == "yellowkey" then
      return 253
    end
  else
    return 249
  end
end

function timeleft()
  if complete or dead then
    return timedone
  end
  return max(maxtime-flr(time()-starttime), 0)
end

function timescore()
  return timedone*10
end

function levelscore()
  return level*500
end


function sprrot(sprite,mode,dx,dy,w,h)
 spr(sprite+mode*2,dx,dy,w,h)
end

function _draw()
  rectfill(0, 0, 128, 128, 0)
  map(px*2-7, py*2-7, 0, 0, 16, 16)
  spr(11, 56, 56, 2, 2)

  for mon in all(monsters) do
    mx=mon[1]
    my=mon[2]
    ms=mon[3]
    mr=mon[4]
    sprrot(ms,mr,56-(px-mx)*16,56-(py-my)*16,2,2)
  end
  
  drawbox(0,112,128,16)
  print("level",3,119,0)
  print("level",2,118,8)
  drawinbox(25,116,14,8)
  printn(level,27,118)
  spr(255,42,116)
  drawinbox(52,116,14,8)
  printnm(timeleft(),54,118,10)
  spr(254,68,116)
  drawinbox(78,116,14,8)
  printn(chips,80,118)
  rectfill(96,112,128,128,0)
  spr(invspr(1),96,112)
  spr(invspr(2),104,112)
  spr(invspr(3),112,112)
  spr(invspr(4),120,112)
  spr(invspr(5),96,120)
  spr(invspr(6),104,120)
  spr(invspr(7),112,120)
  spr(invspr(8),120,120)
  
  local m=gettile(px,py)
  if m==13 then
    local hint = hints[px][py]
    drawinbox(4,84,120,#hint*8)
    for i,h in pairs(hint) do
      print(h,6,78+i*8,12)
    end
  end
  
  if complete then
    rectfill(14,14,112,80,1)
    print("level complete!", 36, 16, 7)
    drawbox(18,24,90,52)
    print("go bit buster!", 38, 26, 0)
    print("time bonus: "..timescore(), 38, 36, 0)
    print("level bonus: "..levelscore(), 34, 42, 0)
    print("level score: "..(timescore()+levelscore()), 34, 48, 0)
    print("total score: "..totalscore, 34, 54, 0)
    drawbox(44, 64, 38, 8)
    print("onward!‚ùé", 46, 66, 0)
  end
  
  if timeup then
    rectfill(20,32,106,70,1)
    print("chip's challenge", 32, 34, 7)
    drawbox(24,42,78,24)
    print("ooops! out of time!", 26, 44,0)
    drawbox(51, 54, 24, 8)
    print("ok ‚ùé", 54, 56, 0)
  end
  
  if dead then
    rectfill(20,32,106,70+#deathmsg*8,1)
    print("chip's challenge", 32, 34, 7)
    drawbox(24,42,78,24+#deathmsg*8)
    print("ooops!", 28, 44,0)
    for i,msg in pairs(deathmsg) do
      print(msg, 28, 44+i*8,0)
    end
    drawbox(51, 54+#deathmsg*8, 24, 8)
    print("ok ‚ùé", 54, 56+#deathmsg*8, 0)
  end
end
