pico-8 cartridge // http://www.pico-8.com
version 42
__lua__
gameover=false
stage=0
sx=0
sy=0
px=32
py=96
pd=0
pt=0
bx=-1
by=-1
bd=-1
enemies={}
ticks=0

function initstage()
	sx=(stage%8)*16
	sy=flr(stage/8)*16
end

function newenemy(x,y,d,t,c,h)
	return {x=x,y=y,d=d,t=t,c=c,h=h,b={x=-1,y=-1,d=-1}}
end

function _init()
	initstage()
	add(enemies,newenemy(0,0,0,0,0,1))
end

function rnd4(x)
	return flr((x+2)>>2)<<2
end

function smget(x,y)
	return mget(x+sx,y+sy)
end

function smset(x,y,s)
	mset(x+sx,y+sy,s)
end

function mget4(x,y)
	local nx=x%2
	local ny=y%2
	local mx=flr(x/2)
	local my=flr(y/2)
	local m=smget(mx,my)
	if(m>=0 and m<=5) then return m end
	local i=1<<(nx+2*ny)
	n=30
	m=m-15
	if(m>=17) then
	 m=m-16
	 n=46
	end
	if(i&m==0) then return 0 end
	return n
end

function mvy(x,y,d)
	y=y+d
	if(y<0 or y>96) then return false end
	local fx=flr(x/4)
	local fy=flr(y/4)
	if(d>0) then fy=fy+2 end
	local m1=mget4(fx,fy)
	local m2=mget4(fx+1,fy)
	return (m1==0 or m1==4 or m2==5) and
		(m2==0 or m2==4 or m2==5)
end

function mvx(x,y,d)
	x=x+d
	if(x<0 or x>96) then return false end
	local fx=flr(x/4)
	local fy=flr(y/4)
	if(d>0) then fx=fx+2 end
	local m1=mget4(fx,fy)
	local m2=mget4(fx,fy+1)
	return (m1==0 or m1==4 or m1==5) and
		(m2==0 or m2==4 or m2==5)
end

function bmvy(x,y,d)
	y=y+d
	if(y<0 or y>104) then return false end
	local fx=flr(x/4)
	local fy=flr(y/4)
	local m1=mget4(fx,fy)
	local m2=mget4(fx-1,fy)
	return (m1==0 or m1==3 or m1==4 or m1==5) and
		(m2==0 or m2==3 or m2==4 or m2==5)
end

function bmvx(x,y,d)
	x=x+d
	if(x<0 or x>104) then return false end
	local fx=flr(x/4)
	local fy=flr(y/4)
	local m1=mget4(fx,fy)
	local m2=mget4(fx,fy-1)
	return (m1==0 or m1==3 or m1==4 or m1==5) and
		(m2==0 or m2==3 or m2==4 or m2==5)
end

function des4(x,y)
	local nx=x%2
	local ny=y%2
	local mx=flr(x/2)
	local my=flr(y/2)
	local m=smget(mx,my)
	if(m==1) then
		mset(mx,my,2)
		gameover=true
		return
	end
	if(m<16 or m>30) then return end
	local i=1<<(nx+2*ny)
	m=(m-15)&(~i)
	if(m>0) then m=m+15 end
	smset(mx,my,m)
end

function des(x,y,d)
	if(x<0 or y<0 or x>104 or y>104) then return end
	local fx=flr(x/4)
	local fy=flr(y/4)
	local fx2=fx
	local fy2=fy
	if(d==0) then
		fx2=fx2-1
	elseif(d==1) then
		fx2=fx2-1
	elseif(d==2) then
		fy2=fy2-1
	elseif(d==3) then
		fy2=fy2-1
	end
	des4(fx,fy)
	des4(fx2,fy2)
end

function efire(e)
	if(e.b.d!=-1) then return end
	e.b.x=e.x
	e.b.y=e.y
	if(e.d==0) then
		e.b.x=e.b.x+4
	elseif(e.d==1) then
		e.b.x=e.b.x+4
		e.b.y=e.b.y+8
	elseif(e.d==2) then
		e.b.x=e.b.x+8
		e.b.y=e.b.y+4
	elseif(e.d==3) then
		e.b.y=e.b.y+4
	end
	e.b.d=e.d
end

function ai(e)
	if(rnd()>0.4) then return end
	local a=flr(rnd(2))
	if(a==0) then
		e.d=flr(rnd(4))
	else
		efire(e)
	end
end

function updatee(e)
	if(e.d==0) then
		e.x=rnd4(e.x)
		if(mvy(e.x,e.y,-1)) then
			e.t=(e.t+1)%2
			e.y=e.y-1
		end
	elseif(e.d==1) then
	 e.x=rnd4(e.x)
	 if(mvy(e.x,e.y,1)) then
		 e.t=(e.t+1)%2
		 e.y=e.y+1
		end
	elseif(e.d==2) then
		e.y=rnd4(e.y)
	 if(mvx(e.x,e.y,1)) then
		 e.t=(e.t+1)%2
		 e.x=e.x+1
		end
	elseif(e.d==3) then
		e.y=rnd4(e.y)
	 if(mvx(e.x,e.y,-1)) then
		 e.t=(e.t+1)%2
		 e.x=e.x-1
		end
	end
end

function updateb(x,y,d)
	if(d==0) then
		if(bmvy(x,y,-2)) then
			y=y-2
		else
			des(x,y-2,d)
			d=-1
		end
	elseif(d==1) then
		if(bmvy(x,y,2)) then
			y=y+2
		else
			des(x,y+2,d)
			d=-1
		end
	elseif(d==2) then
		if(bmvx(x,y,2)) then
			x=x+2
		else
			des(x+2,y,d)
			d=-1
		end
	elseif(d==3) then
		if(bmvx(x,y,-2)) then
		 x=x-2
	 else
			des(x-2,y,d)
	 	d=-1
	 end
	end
	return {x,y,d}
end

function _update()
	if(gameover) then
		if(btnp(‚ùé) or btnp(üÖæÔ∏è)) then
			run()
		end
		return
	end
	if(btn(‚¨ÜÔ∏è)) then
		pd=0
		px=rnd4(px)
		if(mvy(px,py,-1)) then
			pt=(pt+1)%2
			py=py-1
		end
	elseif(btn(‚¨áÔ∏è)) then
	 pd=1
	 px=rnd4(px)
	 if(mvy(px,py,1)) then
		 pt=(pt+1)%2
		 py=py+1
		end
	elseif(btn(‚û°Ô∏è)) then
	 pd=2
		py=rnd4(py)
	 if(mvx(px,py,1)) then
		 pt=(pt+1)%2
		 px=px+1
		end
	elseif(btn(‚¨ÖÔ∏è)) then
	 pd=3
		py=rnd4(py)
	 if(mvx(px,py,-1)) then
		 pt=(pt+1)%2
		 px=px-1
		end
	end
	if(btnp(‚ùé) and bd<0) then
		bx=px
		by=py
		if(pd==0) then
			bx=bx+4
		elseif(pd==1) then
			bx=bx+4
			by=by+8
		elseif(pd==2) then
			bx=bx+8
			by=by+4
		elseif(pd==3) then
			by=by+4
		end
		bd=pd
	end
	local dead={}
	if(bd!=-1) then
		local b=updateb(bx,by,bd)
		bx=b[1]
		by=b[2]
		bd=b[3]
		foreach(enemies,function(e)
			if(bx>=e.x and by>=e.y and
					bx<=e.x+16 and by<=e.y+16) then
					e.h=e.h-1
					if(e.h==0) then
					  add(dead,e)
					end
					bd=-1
			end
		end)
	end
	foreach(dead,function(e)
			del(enemies,e)
	end)
	if(ticks%10==0) then
		foreach(enemies,function(e)	
			ai(e)
		end)
	end
	foreach(enemies,function(e)
		updatee(e)
		if(e.b.d!=-1) then
			local b2=updateb(e.b.x,e.b.y,e.b.d)
			e.b.x=b2[1]
			e.b.y=b2[2]
			e.b.d=b2[3]
		end
	end)
	if(btnp(üÖæÔ∏è)) then
	  stage=stage+1
	  initstage()
	end
	ticks=ticks+1
end

function tspr(d,t,c)
	return 48+c+flr(d/2)*2+t
end

function pspr()
	return tspr(pd,pt,0)
end

function _draw()
 rectfill(0,0,128,128,5)
 rectfill(1,22,106,126,0)
	map(sx,sy,2,23,13,13)
	spr(pspr(),px+2,py+23,1,1,pd==3,pd==1)
	foreach(enemies,function(e)
		spr(tspr(e.d,e.t,4+e.c*4),e.x+2,e.y+23,1,1,e.d==3,e.d==1)
		if(e.b.d!=-1) then
			rectfill(e.b.x+1,e.b.y+22,e.b.x+2,e.b.y+23,13)
		end
	end)
	if(bd!=-1) then
		rectfill(bx+1,by+22,bx+2,by+23,13)
	end
	map(sx,sy,2,23,13,13,0x1)
	if(gameover) then
		print("game", 46, 64, 8)
		print("over", 46, 72, 8)
	end
end
